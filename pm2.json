{
  "name": "news",
  "script": "./server.js",
  "cwd": "./",
  "watch": [
    "components",
    "pages",
    "utils",
    "static",
    "server.js"
  ],
  "ignore_watch": [
    "node_modules"
  ],
  "instances": "max",
  "exec_mode": "cluster_mode",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "./logs/app.err.log",
  "out_file": "./logs/app.out.log",
  "env": {
    "NODE_ENV": "production",
    "PORT": 8001
  }
}
